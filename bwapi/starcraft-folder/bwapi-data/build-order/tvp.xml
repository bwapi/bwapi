    <build name="Terran Barracks" place="barracks"/>
    <invent name="Lockdown" priority="6"/>
    <build name="Terran Barracks" place="barracks"/>
    <invent name="EMP Shockwave" priority="9"/>
    <invent name="Personnel Cloaking" priority="5"/>
